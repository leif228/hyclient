package com.eyunda.third.domain.enumeric;

public enum ApplyStatusCode {
	apply("申请", "申请"), approve("批准", "好友"), reject("驳回", "拒绝");

	private String description;
	private String remark;

	private ApplyStatusCode(String description, String remark) {
		this.description = description;
		this.remark = remark;
	}

	public String getDescription() {
		return description;
	}

	public String getRemark() {
		return remark;
	}
}
