<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="#F7F7F7"
    android:gravity="center_vertical"
    android:paddingBottom="15dp"
    android:paddingLeft="16px"
    android:paddingRight="16px"
    android:paddingTop="15dp" >
    
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_weight="1"
        android:orientation="horizontal" >
        <!-- 船logo -->
	    <ImageView
	        android:id="@+id/boatLogo"
	        android:layout_marginTop="5dp"
	        android:layout_width="50dp"
	        android:layout_height="50dp" />
	    <!-- 船logo结束 -->
		<!-- 文字 -->
	    <LinearLayout
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="5dp"
	        android:layout_weight="1"
	        android:descendantFocusability="blocksDescendants"
	        android:orientation="vertical" >
	        <!-- 文字行一 -->
		    <LinearLayout
		        android:layout_width="fill_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginLeft="5dp"
		        android:layout_weight="1"
		        android:orientation="horizontal" >
		        <TextView
		            android:id="@+id/orderId"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:visibility="gone"
		            />
		      <TextView
		            android:id="@+id/cargoSize"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:visibility="gone"
		            />
	            <TextView
	                android:id="@+id/shipName"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_weight="1"
	                android:textColor="#464646"
	                android:textSize="18dp"
	                />
	            <TextView
	                android:id="@+id/orderNum"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_weight="1"
	                android:textColor="#464646"
	                android:textSize="14dp" 
	                />

		    </LinearLayout>
		    <!-- 文字行一结束 -->
	        <!-- 文字行二 -->
		    <LinearLayout
		        android:layout_width="fill_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginLeft="5dp"
		        android:layout_marginTop="5dp"
		        android:layout_weight="1"
		        android:orientation="horizontal" >
		        <TextView
		            android:id="@+id/daLiRen"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
	                android:textColor="#464646"
	                android:layout_weight="1"
	                android:textSize="14dp"
	                android:visibility="gone"
		            android:text="代理人：王五"
		            />
		       <TextView
		            android:id="@+id/orderCount"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
	                android:textColor="#464646"
	                android:layout_weight="1"
	                android:textSize="14dp"
		            android:text="金额"
		            />
		        <TextView
		            android:id="@+id/orderTime"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_weight="1"
	                android:textColor="#464646"
		            android:textSize="14dp" />
		    </LinearLayout>
		    <!-- 文字行二结束 -->
	        <!-- 文字行三 -->
		    <LinearLayout
		        android:layout_width="fill_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginLeft="5dp"
		        android:layout_marginTop="5dp"
		        android:layout_weight="1"
		        android:orientation="horizontal" >
		        <TextView
		            android:id="@+id/hangXian"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textColor="#464646"
		            android:textSize="14dp" 
		            android:text="珠江888广州-东莞"
		            />
		        <TextView
		            android:id="@+id/shipId"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textColor="#464646"
		            android:textSize="14dp" 
		           android:visibility="gone"
		            />
		        <TextView
		            android:id="@+id/account"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textColor="#464646"
		            android:textSize="14dp" 
		           android:visibility="gone"
		            />
		        <TextView
		            android:id="@+id/shipImage"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textColor="#464646"
		            android:textSize="14dp" 
		           android:visibility="gone"
		            />
		    </LinearLayout>
		    <!-- 文字行三结束 -->
	        <!-- 文字行四 -->
		    <LinearLayout
		        android:layout_width="fill_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginLeft="5dp"
		        android:layout_marginTop="5dp"
		        android:orientation="horizontal" >
	            <TextView
	                android:id="@+id/tuoYunRen"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_weight="1"
	                android:textColor="#464646"
	                android:textSize="14dp" 
	                android:text="托运人：张三"
	                />

	            <TextView
	                android:id="@+id/chenYunRen"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_weight="1"
	                android:textColor="#464646"
	                android:textSize="14dp" 
	                android:text="承运人：李四"
	                />
		    </LinearLayout>
		    <!-- 文字行四结束 -->
	        <!-- 文字行五 -->

	       	<LinearLayout
		        android:layout_width="fill_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginLeft="5dp"
		        android:layout_marginTop="5dp"
		        android:orientation="horizontal" 
		        android:visibility="gone">
	            <TextView
	                android:id="@+id/orderNumx"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_weight="1"
	                android:textColor="#464646"
	                android:textSize="14dp" 
	                android:text="船名:"
	                />
		    </LinearLayout>

		    <!-- 文字行五结束 -->
	        <!-- 状态及操作按钮 -->
		    <LinearLayout
		        android:layout_width="fill_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginLeft="5dp"
		        android:layout_marginTop="10dp"
		        android:layout_weight="1"
		        android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/orderStatic"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textColor="#c8c8c8"
		            android:layout_weight="1"
		            android:textSize="14dp"
		            android:text="未签"
		            />
		        <Button
		            android:id="@+id/btnEdit"
		            android:layout_width="60dp"
		            android:layout_height="25dp"
		            android:layout_marginRight="5dp"
		            android:background="@drawable/eyd_btn_style_positive"
		            android:textSize="14dp"
		            android:textColor="#ffffff"
		            android:text="编辑" />
		        <Button
		            android:id="@+id/btnSign"
		            android:layout_width="80dp"
		            android:layout_height="25dp"
		            android:layout_marginRight="5dp"
		            android:background="@drawable/eyd_btn_style_calm"
		            android:textSize="14dp"
		            android:textColor="#ffffff"
		            android:text="签名" />
		        <Button
		            android:id="@+id/btnMonitor"
		            android:layout_width="60dp"
		            android:layout_height="25dp"
		            android:textSize="14dp"
		            android:textColor="#4C7DD3"
		            android:layout_marginRight="5dp"
		            android:background="@drawable/eyd_btn_style_calm"
		            android:visibility="gone"
		            android:text="监控" />
		        <Button
		            android:id="@+id/btnPay"
		            android:layout_width="80dp"
		            android:layout_height="25dp"
		            android:textSize="14dp"
		            android:textColor="#4C7DD3"
		            android:layout_marginRight="5dp"
		            android:background="@drawable/eyd_btn_style_energized"
		            android:text="支付" />
		        <Button
		            android:id="@+id/btnRefund"
		            android:layout_width="80dp"
		            android:layout_height="25dp"
		            android:textSize="14dp"
		            android:textColor="#4C7DD3"
		            android:visibility="gone"
		            android:layout_marginRight="5dp"
		            android:background="@drawable/eyd_btn_style_energized"
		            android:text="退款" />
		        <Button
		            android:id="@+id/btnDel"
		            android:layout_width="60dp"
		            android:layout_height="25dp"
		            android:textSize="14dp"
		            android:textColor="#4C7DD3"
		            android:background="@drawable/eyd_btn_style_energized"
		            android:visibility="gone"
		            android:text="删除" />

		    </LinearLayout>
		    <!-- 文字行二结束 -->
    	</LinearLayout>
    	<!-- 文字结束 -->
    </LinearLayout>
    <!-- 右部图标 -->
    <LinearLayout
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:gravity="center_vertical" 
        android:visibility="gone">

        <ImageView
            android:id="@+id/but3"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:focusable="false"
            android:src="@drawable/home_arrow" />
    </LinearLayout>
	<!-- 右部结束 -->
</LinearLayout>