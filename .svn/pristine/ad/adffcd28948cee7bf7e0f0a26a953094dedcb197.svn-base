<!-- 首页船舶列表布局 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:focusable="false"
    android:focusableInTouchMode="false"
    android:orientation="horizontal" 
    android:background="#BFBFBF"
    >
<!-- android:background="@drawable/bbuton_default_rounded"   -->

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingLeft="10dp"
        android:paddingTop="10dp" 
        android:paddingBottom="10dp"
        android:layout_weight="9"
        android:baselineAligned="false" >

        <!-- 船舶logo -->

       

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:gravity="center_vertical"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <!-- 船舶名称 -->

                <TextView
                    android:id="@+id/loginName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:maxLines="2"
                    android:text="登录名"
                    android:textColor="#FF000000"/>
                <!-- 推荐图标 -->

                <ImageView
                    android:id="@+id/jian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/home_jian"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:visibility="gone" />
                <!-- 船舶ID -->

                <TextView
                    android:id="@+id/boat_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:text="1111"
                    android:visibility="gone" />

                <!-- 载货量 -->

                <TextView
                    android:id="@+id/name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:gravity="right"
                    android:maxLines="2"
                    android:text="姓名：5000吨"
                    android:textColor="#FF5D5D5D"
                    android:textSize="10.0dp"
    				android:visibility="gone"
    				/>
            </LinearLayout>
            <!-- 船舶描述 -->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/boat_dis"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:maxLines="2"
                    android:text="email:"
                    android:visibility="gone"
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <!-- 航线报价 -->
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" 
                android:visibility="gone">

                <TextView
                    android:id="@+id/companyName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:maxLines="2"
                    android:text="公司名:"
                    android:textColor="#FF5D5D5D"
                    android:textSize="10.0dp" />

                <TextView
                    android:id="@+id/boat_time"
                    android:layout_width="136dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:visibility="gone"
                    android:focusable="false"
                    android:gravity="right"
                    android:textColor="#464646"
                    android:textSize="13dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/areas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:maxLines="2"
                    android:text="地区:"
                    android:textColor="#464646"
                    android:textSize="14dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/price"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" 
                android:visibility="gone">

                <!-- 船舶报价 -->

                <TextView
                    android:id="@+id/daiLi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:text="代理船舶数"
                    android:textColor="#FF5D5D5D"
                    android:textSize="10.0dp" />

                <TextView
                    android:id="@+id/cjht"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:gravity="right"
                    android:text="成交合同数"
                    android:textColor="#FF5D5D5D"
                    android:textSize="10.0dp" />

                <TextView
                    android:id="@+id/isCreate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:visibility="gone" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <ImageView
 	    android:id="@+id/down_arrow_img"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:paddingTop="10.0dip"
        android:paddingRight="20.0dip"
        android:paddingLeft="20dip"
        android:src="@drawable/categories_down_arrow"
        android:layout_weight="1"
        android:clickable="true"
        android:contentDescription="业务员"
        />

</LinearLayout>