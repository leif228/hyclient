<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homePage"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/bgColor"
    android:orientation="vertical" >

    <include layout="@layout/normal_top" />

    <LinearLayout
        android:id="@+id/shipImg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" 
        android:visibility="gone">

        <LinearLayout
            android:id="@+id/shipImg3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/img31"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@drawable/home_default"
                    android:clickable="true"
                    android:scaleType="fitXY" />

                <ImageView
                    android:id="@+id/img32"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@drawable/home_default"
                    android:clickable="true"
                    android:scaleType="fitXY" />

                <ImageView
                    android:id="@+id/img33"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@drawable/home_default"
                    android:clickable="true"
                    android:scaleType="fitXY" />
            </LinearLayout>

            <!-- android:background="@drawable/bbuton_default_rounded" -->

            <LinearLayout
                android:id="@+id/shipInfo3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/ll_1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/ship_jj1"
                    android:orientation="horizontal"
                    android:paddingLeft="5dp"
                    android:paddingTop="2dp" >

                    <TextView
                        android:id="@+id/tv31"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="航次:"
                        android:textColor="#464646"
                        android:textSize="14dp" />

                    <TextView
                        android:id="@+id/tv32"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="时间:"
                        android:textColor="#464646"
                        android:textSize="14dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_2"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/ship_jj2"
                    android:orientation="horizontal"
                    android:paddingLeft="5dp"
                    android:paddingTop="2dp" >

                    <TextView
                        android:id="@+id/tv33"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="起运港:"
                        android:textColor="#464646"
                        android:textSize="14dp" />

                    <TextView
                        android:id="@+id/tv34"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="到达港:"
                        android:textColor="#464646"
                        android:textSize="14dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_3"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/ship_jj1"
                    android:orientation="horizontal"
                    android:paddingLeft="5dp"
                    android:paddingTop="2dp"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/tv35"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="参考航时:"
                        android:textColor="#464646"
                        android:textSize="14dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/tv36"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="运行航时:"
                        android:textColor="#464646"
                        android:textSize="14dp"
                        android:visibility="gone" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/ship_jj2"
                    android:orientation="horizontal"
                    android:paddingLeft="5dp"
                    android:paddingTop="2dp"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/tv37"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="参考里程:"
                        android:textColor="#464646"
                        android:textSize="14dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/tv38"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="运行里程:"
                        android:textColor="#464646"
                        android:textSize="14dp"
                        android:visibility="gone" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_5"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/ship_jj1"
                    android:orientation="horizontal"
                    android:paddingLeft="5dp"
                    android:paddingTop="2dp" >

                    <TextView
                        android:id="@+id/tv_lat3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="航向: "
                        android:textColor="#464646"
                        android:textSize="14dp" />

                    <TextView
                        android:id="@+id/tv_speed3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="航速: "
                        android:textColor="#464646"
                        android:textSize="14dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_6"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/ship_jj2"
                    android:orientation="horizontal"
                    android:paddingLeft="5dp"
                    android:paddingTop="2dp" >

                    <TextView
                        android:id="@+id/tv_j3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="经度: "
                        android:textColor="#464646"
                        android:textSize="14dp" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tv_w3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="纬度: "
                            android:textColor="#464646"
                            android:textSize="14dp" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom"
                            android:src="@drawable/up" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/shipImg4"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:baselineAligned="false"
            android:orientation="horizontal" 
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/shipInfo4"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:clickable="true"
                android:gravity="center_vertical"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/tv41"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj1"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="航次:"
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv42"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj2"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="起运港:"
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv43"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj1"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="到达港:"
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv44"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj2"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="参考航时:"
                    android:textColor="#464646"
                    android:textSize="14dp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv45"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj1"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="运行航时:"
                    android:textColor="#464646"
                    android:textSize="14dp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv46"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj2"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="参考里程:"
                    android:textColor="#464646"
                    android:textSize="14dp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv47"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj1"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="运行里程:"
                    android:textColor="#464646"
                    android:textSize="14dp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv48"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj2"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="时间:"
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv_lat4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj1"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="航向: "
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv_speed4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj2"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="航速: "
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv_j4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj1"
                    android:gravity="center_vertical"
                    android:paddingLeft="2dp"
                    android:text="经度: "
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj2"
                    android:orientation="horizontal"
                    android:paddingLeft="2dp" >

                    <TextView
                        android:id="@+id/tv_w4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:text="纬度: "
                        android:textColor="#464646"
                        android:textSize="14dp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:src="@drawable/up" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/shipName"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/navitemlayout"
                    android:gravity="center_horizontal"
                    android:text=""
                    android:visibility="gone" />

                <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/img1"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:background="@drawable/home_default"
                            android:clickable="true"
                            android:scaleType="fitXY" />

                        <ImageView
                            android:id="@+id/img2"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:background="@drawable/home_default"
                            android:clickable="true"
                            android:scaleType="fitXY" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/img3"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:background="@drawable/home_default"
                            android:clickable="true"
                            android:scaleType="fitXY" />

                        <ImageView
                            android:id="@+id/img4"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:background="@drawable/home_default"
                            android:clickable="true"
                            android:scaleType="fitXY" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <com.baidu.mapapi.map.MapView
            android:id="@+id/mapHistory"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:clickable="true" />

        <RelativeLayout
            android:id="@+id/history"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="50dp"
            android:layout_marginRight="30dp"
            android:background="@drawable/navlayout"
            android:clickable="true"
            android:gravity="center" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="回放" />
        </RelativeLayout>
    </RelativeLayout>

</LinearLayout>