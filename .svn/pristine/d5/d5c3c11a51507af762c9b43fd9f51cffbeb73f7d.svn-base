<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rlHome"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >

    <com.baidu.mapapi.map.MapView
        android:id="@+id/mapHistory"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/shipImg"
        android:clickable="true" />

    <LinearLayout
        android:id="@+id/llNav"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/navlayout"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:visibility="gone" >

        <LinearLayout
            android:id="@+id/shipDyn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_weight="8"
            android:background="@drawable/navitemlayout"
            android:gravity="center" >

            <EditText
                android:id="@+id/search_et"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:hint="搜索船舶"
                android:singleLine="true" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/search_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/search_bg_home" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/shipMoniter"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="100dp"
        android:layout_marginLeft="3dp"
        android:background="@drawable/navlayout"
        android:baselineAligned="false"
        android:clickable="true"
        android:gravity="center"
        android:orientation="horizontal"
        android:visibility="gone" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_marker"
            android:clickable="true" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/shipImg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/llNav"
        android:orientation="vertical"
        android:visibility="gone" >

        <LinearLayout
            android:id="@+id/shipImg3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/img31"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@drawable/home_default"
                    android:clickable="true"
                    android:scaleType="fitXY" />

                <ImageView
                    android:id="@+id/img32"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@drawable/home_default"
                    android:clickable="true"
                    android:scaleType="fitXY" />

                <ImageView
                    android:id="@+id/img33"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@drawable/home_default"
                    android:clickable="true"
                    android:scaleType="fitXY" />
            </LinearLayout>

            <!-- android:background="@drawable/bbuton_default_rounded" -->

            <LinearLayout
                android:id="@+id/shipInfo3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/navlayout3"
                android:clickable="true"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/ship_jj1"
                    android:orientation="horizontal"
                    android:paddingBottom="2dp"
                    android:paddingLeft="5dp"
                    android:paddingTop="2dp" >

                    <TextView
                        android:id="@+id/tv_lat3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:text="航向:未知"
                        android:textColor="#464646"
                        android:textSize="14dp" />

                    <TextView
                        android:id="@+id/tv_speed3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="8"
                        android:text="航速:未知"
                        android:textColor="#464646"
                        android:textSize="14dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/ship_jj2"
                    android:orientation="horizontal"
                    android:paddingBottom="2dp"
                    android:paddingLeft="5dp"
                    android:paddingTop="2dp" >

                    <TextView
                        android:id="@+id/tv_j3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:text="经度:未知"
                        android:textColor="#464646"
                        android:textSize="14dp" />

                    <TextView
                        android:id="@+id/tv_w3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="8"
                        android:text="纬度:未知"
                        android:textColor="#464646"
                        android:textSize="14dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/ship_jj1"
                    android:orientation="horizontal"
                    android:paddingBottom="2dp"
                    android:paddingLeft="5dp"
                    android:paddingTop="2dp" >

                    <TextView
                        android:id="@+id/tv_zz3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:text="装载量:0吨"
                        android:textColor="#464646"
                        android:textSize="14dp" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="8"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tv_time3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="最后时间:未知"
                            android:textColor="#464646"
                            android:textSize="14dp" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom"
                            android:src="@drawable/up" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/shipImg4"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:baselineAligned="false"
            android:orientation="horizontal"
            android:visibility="gone" >

            <LinearLayout
                android:id="@+id/shipInfo4"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_weight="1"
                android:background="@drawable/navlayout3"
                android:clickable="true"
                android:gravity="center_vertical"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/tv_lat4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj1"
                    android:gravity="center_vertical"
                    android:paddingLeft="5dp"
                    android:text="航向:未知"
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv_speed4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj2"
                    android:gravity="center_vertical"
                    android:paddingLeft="5dp"
                    android:text="航速:未知"
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv_j4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj1"
                    android:gravity="center_vertical"
                    android:paddingLeft="5dp"
                    android:text="经度:未知"
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv_w4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj2"
                    android:gravity="center_vertical"
                    android:paddingLeft="5dp"
                    android:text="纬度:未知"
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv_zz4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj1"
                    android:gravity="center_vertical"
                    android:paddingLeft="5dp"
                    android:text="装载量:0吨 "
                    android:textColor="#464646"
                    android:textSize="14dp" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/ship_jj2"
                    android:orientation="horizontal"
                    android:paddingLeft="5dp" >

                    <TextView
                        android:id="@+id/tv_time4"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:text="最后时间:未知"
                        android:textColor="#464646"
                        android:textSize="14dp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:src="@drawable/up" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/shipName"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/navitemlayout"
                    android:gravity="center_horizontal"
                    android:text=""
                    android:visibility="gone" />

                <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/img1"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:background="@drawable/home_default"
                            android:clickable="true"
                            android:scaleType="fitXY" />

                        <ImageView
                            android:id="@+id/img2"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:background="@drawable/home_default"
                            android:clickable="true"
                            android:scaleType="fitXY" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/img3"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:background="@drawable/home_default"
                            android:clickable="true"
                            android:scaleType="fitXY" />

                        <ImageView
                            android:id="@+id/img4"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:background="@drawable/home_default"
                            android:clickable="true"
                            android:scaleType="fitXY" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <ImageView 
        android:id="@+id/gps"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_above="@+id/llNav1"
        android:contentDescription="定位"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="30dp"
        android:src="@drawable/gps"
        android:visibility="gone"/>
    
    <LinearLayout
        android:id="@+id/llNav1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="50dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:baselineAligned="false"
        android:orientation="horizontal" >

        <RelativeLayout
            android:id="@+id/me"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/navlayout"
            android:clickable="true"
            android:gravity="center" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="设置" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/group"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/navlayout"
            android:clickable="true"
            android:gravity="center" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="航次" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/msg"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/navlayout"
            android:clickable="true" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="报警" />

            <FrameLayout
                android:id="@+id/unread_msg_number_fl"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="0dp" >

                <TextView
                    android:id="@+id/unread_msg_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:background="@drawable/eyd_chat_red_circle"
                    android:gravity="center"
                    android:text="30"
                    android:textColor="@android:color/white"
                    android:textSize="12sp"
                    android:visibility="gone" />
            </FrameLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/history"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:background="@drawable/navlayout"
            android:clickable="true"
            android:gravity="center" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="回放" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/llNav2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="50dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:background="@drawable/navlayout"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:visibility="gone" >

        <LinearLayout
            android:id="@+id/shipls"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/navitemlayout"
            android:gravity="center" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="历史回放" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/shipzx"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/navitemlayout"
            android:gravity="center" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="执行中航次" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/shipgroupsz"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="分组查看" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/shipMoniter2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:visibility="gone" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="船舶监控" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>